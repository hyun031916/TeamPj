<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pjmanagement">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".MainActivity3"
            android:label="@string/title_activity_main3"></activity>
        <activity android:name=".TapActivity" />
        <activity android:name=".CalendarTableActivity" />
        <activity android:name=".MakeProjectActivity" />
        <activity android:name=".TimeTableActivity" />
        <activity
            android:name=".JoinActivity"
            android:theme="@style/AppTheme.NoActionbar" />
        <activity android:name=".ChatActivity" />
        <activity android:name=".PlanningActivity" />
        <activity android:name=".CheckMemberActivity" />
        <activity android:name=".NewProjectActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".MainActivity2"
            android:theme="@style/AppTheme.NoActionbar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CalendarActivity" />
        <meta-data
            android:name="com.google.firebase.nl.vision.DEPEDENCIES"
            android:value="face" />
    </application>

</manifest>